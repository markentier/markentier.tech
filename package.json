{
  "name": "markentier.tech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:lambda": "netlify-lambda serve ./functions",
    "build:lambda": "netlify-lambda build ./functions"
  },
  "proxy": {
    "/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\functions": ""
      }
    }
  },
  "dependencies": {
    "netlify-lambda": "*"
  },
  "devDependencies": {
    "gulp": "^4.0",
    "gulp-cli": "*",
    "gulp-htmlmin": "*",
    "gulp-htmltidy": "*",
    "gulp-jsonminify": "*",
    "gulp-minify": "*",
    "gulp-posthtml": "*",
    "posthtml-align-style": "./tools/packages/posthtml-align-style",
    "posthtml-alt-always": "*",
    "posthtml-avif-webp": "./tools/packages/posthtml-avif-webp",
    "posthtml-img-autosize": "./vendor/posthtml-img-autosize",
    "text-to-svg": "*"
  },
  "peerDependencies": {
    "image-size": "*"
  }
}
