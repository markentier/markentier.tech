{
  "name": "markentier.tech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:lambda": "netlify-lambda serve ./functions",
    "build:lambda": "netlify-lambda build ./functions"
  },
  "proxy": {
    "/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\functions": ""
      }
    }
  },
  "dependencies": {
    "@sendgrid/mail": "*",
    "netlify-lambda": "*",
    "request": "*"
  },
  "devDependencies": {
    "eslint-config-standard": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": "*",
    "image-size": "*",
    "post-config": "*",
    "post-load-plugins": "*",
    "posthtml": "*",
    "posthtml-alt-always": "*",
    "posthtml-cli": "*",
    "posthtml-img-autosize": "./vendor/posthtml-img-autosize",
    "posthtml-load-config": "*",
    "sqip": "*"
  },
  "peerDependencies": {},
  "posthtml": {
    "from": "public/**/*.html",
    "plugins": {
      "img-autosize": {
        "root": "public",
        "processEmptySize": true
      }
    }
  }
}
