{
  "name": "markentier.tech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:lambda": "netlify-lambda serve ./functions",
    "build:lambda": "netlify-lambda build ./functions"
  },
  "proxy": {
    "/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\functions": ""
      }
    }
  },
  "dependencies": {
    "netlify-lambda": "^2.0.3"
  },
  "devDependencies": {
    "cssnano": "*",
    "gulp": "^4.0",
    "gulp-cli": "*",
    "gulp-htmlmin": "*",
    "gulp-htmltidy": "*",
    "gulp-jsonminify": "*",
    "gulp-minify": "*",
    "gulp-postcss": "*",
    "gulp-posthtml": "*",
    "htmlnano": "*",
    "postcss": "*",
    "posthtml-align-style": "file:./tools/packages/posthtml-align-style",
    "posthtml-alt-always": "*",
    "posthtml-avif-webp": "file:./tools/packages/posthtml-avif-webp",
    "posthtml-externalize-styles": "file:./tools/packages/posthtml-externalize-styles",
    "posthtml-img-autosize": "file:./vendor/posthtml-img-autosize",
    "text-to-svg": "*"
  },
  "peerDependencies": {
    "image-size": "*"
  }
}
