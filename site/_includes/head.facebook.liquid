<meta property="og:url" content="{{canonical_url}}">
<meta property="og:title" content="{{page.title}}">
<meta property="og:description" content="{{page.content | strip_html | truncatewords: 23 }}">
<meta property="og:image" content="{{page.data.image_url}}">
<meta property="og:site_name" content="{{site.title}}">
<meta property="og:see_also" content="{{site.base_url}}">
